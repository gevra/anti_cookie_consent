{
  "name": "Anti Cookie consent",
  "version": "0.0.1.1",
  "description": "removes the cookie consent form and re-enables scrolling",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["/jquery.3.4.1.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "permissions": ["activeTab"]
}
